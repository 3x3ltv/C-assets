#include <stdio.h>
#include <stdlib.h>

int main()
{
    int arr[6];
    for(int i=0;i<6;i++)
    {
        scanf("%i",&arr[i]);
    }
     for(int i=0;i<6;i++)
    {
        printf("%i ",arr[i]);
    }
    int farr[2][2];
    int sarr[2][2];
        farr[0][0]=1;
        farr[0][1]=0;
        farr[1][0]=1;
        farr[1][1]=4;
        sarr[0][0]=1;
        sarr[0][0]=2;
        sarr[0][0]=0;
        sarr[0][0]=1;
    int r;
    for(int i=0;i<2;i++)
    {
        for(int j=0;j<2;j++)
        {
                    r=(farr[i][j]*sarr[i][j]);
                    printf(" %i",r);
        }
    }
    return 0;
}
